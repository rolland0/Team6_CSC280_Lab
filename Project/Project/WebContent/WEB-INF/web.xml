<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Project</display-name>
  <welcome-file-list>
    <welcome-file>Setup</welcome-file>
  </welcome-file-list>
  <security-constraint>
    <display-name>Login Pages</display-name>
    <web-resource-collection>
      <web-resource-name>loginPages</web-resource-name>
      <url-pattern>/WEB-INF/loginPages/*</url-pattern>
      <http-method>GET</http-method>
      <http-method>POST</http-method>
    </web-resource-collection>
    <user-data-constraint>
      <transport-guarantee>CONFIDENTIAL</transport-guarantee>
    </user-data-constraint>
  </security-constraint>
  <login-config>
    <auth-method>FORM</auth-method>
    <form-login-config>
      <form-login-page>/WEB-INF/loginPages/login.jsp</form-login-page>
      <form-error-page>/WEB-INF/loginPages/loginError.jsp</form-error-page>
    </form-login-config>
  </login-config>
  <error-page>
    <error-code>403</error-code>
    <location>/WEB-INF/loginPages/loginError.jsp</location>
  </error-page>
  <error-page>
    <error-code>401</error-code>
    <location>/WEB-INF/loginPages/loginError.jsp</location>
  </error-page>
</web-app>